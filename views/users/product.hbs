<body>


	<div class="page-content">
		<div class="container">
			<div class="row">
				<div class="col-lg-9">
					<div class="toolbox">
						
						
					</div><!-- End .toolbox -->

					<div class="products mb-3">

						<div class="row justify-content-center ">
							{{#each products }}
							<div class="col-6 col-md-4 col-lg-4">
								<div class="product product-7 text-center">
									<figure class="product-media">
										<span class="product-label label-new">New</span>
										<a href="/productview/{{this._id}}">
											<img src="/images/product/{{this.img.[0]}}" alt="Product image"
												class="product-image" style="width: 250px; height:300px;">
										</a>

										<div class="product-action-vertical">
											<a href="/addToWishlist/{{this._id}}"
												class="btn-product-icon btn-wishlist btn-expandable"><span>add to
													wishlist</span></a>
											
										</div><!-- End .product-action-vertical -->

										<div class="product-action">
											<a href="/addToCart/{{this._id}}" class="added btn-product btn-cart"
												title="Add to cart"><span>add To cart</span></a>
										</div><!-- End .product-action -->
									</figure><!-- End .product-media -->

									<div class="product-body">
										<div class="product-cat">
											<a href="#"></a>
										</div><!-- End .product-cat -->
										<h3 class="product-title"><a href=""></a>{{this.phone}}</h3>
										<!-- End .product-title -->
										<div class="product-price">

											{{#if this.Percentage}}
											<span>₹{{this.offPrice}}&nbsp;&nbsp;</span>
											₹ <s>{{this.originalPrice}} </s>
											<span>&nbsp;&nbsp;{{this.Percentage}}%</span>
											{{else}}
											₹{{this.price}}
											{{/if}}

										</div><!-- End .product-price -->
										

										<div class="product-nav product-nav-thumbs">
											<a href="#" class="active">
												<img src="/images/product/{{this.img.[1]}}" alt="product desc"
													style="width: 60px; height: 50px;">
											</a>
											<a href="#">
												<img src="/images/product/{{this.img.[2]}}" alt="product desc"
													style="width: 60px; height: 50px;">
											</a>

											<a href="#">
												<img src="/images/product/{{this.img.[3]}}" alt="product desc"
													style="width: 60px; height: 50px;">
											</a>
										</div><!-- End .product-nav -->
									</div><!-- End .product-body -->
								</div><!-- End .product -->
							</div><!-- End .col-sm-6 col-lg-4 -->
							{{/each}}
						</div><!-- End .row -->
					</div><!-- End .products -->

					<nav aria-label="Page navigation" style="margin-left: 240px;">
							    <ul class="pagination">
									{{#ifEquals '1' currentPage}}
							        <li class="page-item disabled" >
							            <a class="page-link page-link-prev" href="/product/?page={{../previous.page}}" aria-label="Previous" tabindex="-1">
							                <span aria-hidden="true"><i class="icon-long-arrow-left"></i></span>Prev
							            </a>
							        </li>
									{{else}}
									 <li class="page-item">
							            <a class="page-link page-link-prev" href="/product/?page={{../previous.page}}" aria-label="Previous" tabindex="-1" >
							                <span aria-hidden="true"><i class="icon-long-arrow-left"></i></span>Prev
							            </a>
							        </li>
									{{/ifEquals}}
									{{#each pages}}
									
							       
									<li class="page-item " aria-current="page"><a class="page-link" href="/product/?page={{this}}">{{this}}</a></li>
									
									{{/each}}
									{{#ifEquals pageCount currentPage }}
							        <li class="page-item disabled" >
							            <a class="page-link page-link-next" href="/product/?page={{../next.page}}" aria-label="Next">
							                Next <span aria-hidden="true"><i class="icon-long-arrow-right"></i></span>
							            </a>
							        </li>
									{{else}}
									 <li class="page-item">
							            <a class="page-link page-link-next" href="/product/?page={{../next.page}}" aria-label="Next">
							                Next <span aria-hidden="true"><i class="icon-long-arrow-right"></i></span>
							            </a>
							        </li>
									{{/ifEquals}}
							    </ul>
							</nav>
				</div><!-- End .col-lg-9 -->
				<aside class="col-lg-3 order-lg-first">
					<div class="sidebar sidebar-shop">
						<div class="widget widget-clean">
							<label>Filters:</label>
							<a href="/product" class="sidebar-filter-clear">Clean All</a>
						</div><!-- End .widget widget-clean -->


						<div class="widget widget-collapsible mt-1 col-3">
							<h3 class="widget-title">
								<a data-toggle="collapse" href="#widget-4" role="button" aria-expanded="true"
									aria-controls="widget-4">
									Brand
								</a>
							</h3><!-- End .widget-title -->

							<div class="collapse show" id="widget-4">
								<div class="widget-body">

									<form action="/filter" method="get">
										{{#each brands}}
										<div class="filter-items">

											<div class="filter-item">
												<div class="custom-control custom-checkbox">
													<input type="checkbox" class="custom-control-input" name="brandName"
														id="brand{{this._id}}" value="{{this._id}}">
													<label class="custom-control-label"
														for="brand{{this._id}}">{{this.category}}</label>
												</div><!-- End .custom-checkbox -->
											</div><!-- End .filter-item -->


										</div><!-- End .filter-items -->
										{{/each}}


										<button type="submit" class=" btn btn-primary">Submit</button>
									</form>
								</div><!-- End .widget-body -->


							</div><!-- End .collapse -->
						</div><!-- End .widget -->

						
					</div><!-- End .sidebar sidebar-shop -->
				</aside><!-- End .col-lg-3 -->
			</div><!-- End .row -->
		</div><!-- End .container -->
	</div><!-- End .page-content -->


